{
  "name": "hall-booking-app",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": ">=20.11.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:payments": "jest --config ./test/jest-payment-integration.json",
    "test:integration": "jest --config ./test/jest-integration.json",
    "test:comprehensive": "jest --config ./test/jest-comprehensive.json",
    "test:all": "npm run test:comprehensive",
    "test:ci": "npm run test:comprehensive -- --coverage --watchAll=false",
    "test:full-coverage": "jest --config ./test/jest-full-coverage.json",
    "test:watch-payments": "jest --config ./test/jest-payment-integration.json --watch",
    "test:watch-integration": "jest --config ./test/jest-integration.json --watch",
    "test:watch-comprehensive": "jest --config ./test/jest-comprehensive.json --watch",
    "test:unit-only": "jest src/**/*.spec.ts --coverage --testPathIgnorePatterns=test/",
    "test:existing-only": "jest src/**/*.spec.ts --coverage",
    "test:new-only": "jest test/**/*.spec.ts test/**/*.e2e-spec.ts --coverage",
    "test:performance": "jest --config ./test/jest-performance.json",
    "test:summary": "echo 'Running all test suites...' && npm run test:existing-only && npm run test:new-only && npm run test:payments && npm run test:e2e",
    "test:validate-all": "npm run test:existing-only && npm run test:integration && npm run test:payments && npm run test:e2e && npm run test:performance",
    "db:migrate": "npx prisma migrate deploy",
    "db:generate": "npx prisma generate",
    "db:studio": "npx prisma studio",
    "db:test-setup": "npm run db:migrate && npm run db:generate"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.2.1",
    "@prisma/client": "^6.18.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dotenv": "^17.2.3",
    "ioredis": "^5.8.2",
    "moment-timezone": "^0.5.45",
    "nest-winston": "^1.10.2",
    "razorpay": "^2.9.4",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.18.3",
    "uuid": "^10.0.0",
    "date-fns": "^3.6.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/moment-timezone": "^0.5.30",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@types/winston": "^2.4.4",
    "@upstash/redis": "^1.35.6",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "prettier": "^3.4.2",
    "prisma": "^6.18.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}